<?xml version="1.0" encoding="UTF-8"?>
<ER:DataBase
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:ER="https://homework4_ecommerce_project/ER"
    name="Ecommerce">
  <entities
      name="Order">
    <attributes
        name="orderID"
        Type="Int"/>
    <attributes
        name="date"
        Type="Date"/>
    <attributes
        name="status"/>
    <attributes
        name="totalAmount"
        Type="Float"/>
    <relations
        name="orderItem"
        entity="//@entities.2"/>
    <relations
        name="payment"
        entity="//@entities.5"/>
    <relations
        name="shipping"
        entity="//@entities.3"/>
    <relations
        name="review"
        entity="//@entities.4"/>
  </entities>
  <entities
      name="Customar">
    <attributes
        name="customarID"
        Type="Int"/>
    <attributes
        name="name"/>
    <attributes
        name="email"/>
    <attributes
        name="address"/>
    <relations
        name="order"
        entity="//@entities.0"/>
  </entities>
  <entities
      name="OrderItem">
    <attributes
        name="quantity"
        Type="Int"/>
    <attributes
        name="price"
        Type="Float"/>
    <relations
        name="product"
        entity="//@entities.6"
        cardinality="ONE2MANY"/>
  </entities>
  <entities
      name="Shipping">
    <attributes
        name="shippingMethod"/>
    <attributes
        name="trackingMethod"/>
    <attributes
        name="shippingDate"
        Type="Date"/>
  </entities>
  <entities
      name="Review">
    <attributes
        name="reviewID"
        Type="Int"/>
    <attributes
        name="rating"
        Type="Float"/>
    <attributes
        name="comment"/>
  </entities>
  <entities
      name="Payment">
    <attributes
        name="paymentID"
        Type="Int"/>
    <attributes
        name="amount"
        Type="Float"/>
    <attributes
        name="method"/>
    <attributes
        name="status"/>
  </entities>
  <entities
      name="Product">
    <attributes
        name="productID"
        Type="Int"/>
    <attributes
        name="name"/>
    <attributes
        name="price"
        Type="Float"/>
    <attributes
        name="description"/>
    <attributes
        name="stockQuantity"
        Type="Int"/>
    <relations
        name="category"
        entity="//@entities.7"
        cardinality="MANY2MANY"/>
  </entities>
  <entities
      name="Category">
    <attributes
        name="categoryID"
        Type="Int"/>
    <attributes
        name="name"/>
    <attributes
        name="parentCategory"/>
  </entities>
  <entities
      name="Cart">
    <relations
        name="product"
        entity="//@entities.6"
        cardinality="ONE2MANY"/>
  </entities>
</ER:DataBase>
